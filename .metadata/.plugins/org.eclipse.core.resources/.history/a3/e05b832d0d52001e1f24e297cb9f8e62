package com.cos.security1.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class IndexController {

	//localhost:8090/
	//localhost:8090
	@GetMapping({"","/"})
	public String index() {
		return "index";
	}
	
	@GetMappring("/user")
	public String user() {
		return "user";
	}
}
